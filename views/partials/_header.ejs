<header>
  &emsp;
  <% if (username) { %>
    <form method="POST" action="/logout"> 
      <%= username %>&emsp;
      <input type="submit" value="Logout"/>
    </form>
  <% } else { %>
  <form method="POST" action="/urls/login">  <!--this just has to match the path on server?-->
    <input type="text" name="login" placeholder="username" style="width: 100px"> <!--what is each for?-->
    <input type="submit" value="Login"/> 
  </form>
  <% } %>
</header>